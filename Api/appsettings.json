{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System.Net.Http.HttpClient.Hello": "Information"
    },
    "GELF": {
      "Host": "127.0.0.1",
      "Port": 12201, // Not required if using default 12201.
      "Protocol": "udp", // Not required if using default UDP.
      "LogSource": "SoapClient" // Not required if set in code as above.      
    }
  },
  "AllowedHosts": "*"
}
